   "source": [
    "from data_processing import get_data_from_pgn\n",
    "\n",
    "X_all, y_all = get_data_from_pgn('data/lichess_elite.pgn', feature_version=1, num_data=50000)\n",
    "\n",
    "from sklearn import svm\n",
    "\n",
    "clf = svm.SVR()\n",
    "\n",
    "clf.fit(X_all, y_all)\n",
    "\n",
    "import pickle\n",
    "\n",
    "pickle.dump(clf, open('models/model.sav', 'wb'))"
   ]
